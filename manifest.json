{
  "manifest_version": 2,
  "name": "Lexilogos Arabic Transcriber",
  "version": "1.0",
  "description": "Converts latin letters to arabic ones",
  "icons": {
      "48": "icon.png"
  },
  "browser_action": {
      "default_icon": {
          "48": "icon.png"
      },
      "default_popup": "popup.html"
  },
  "permissions": [],
  "background": {
      "scripts": ["popup.js"]
  }
}
